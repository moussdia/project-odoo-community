<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- séquence client Publiciataire-->
        <record id="sequence_partner_pub" model="ir.sequence">
            <field name="name">sequence.partner.pub</field>
            <field name="code">pub.partner</field>
            <field name="prefix">P/</field>
			<field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- séquence client Imprimerie-->
        <record id="sequence_partner_imp" model="ir.sequence">
            <field name="name">Sequence.partner.imp</field>
            <field name="code">imp.partner</field>
            <field name="prefix">I/</field>
			<field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <!-- séquence client Distribution-->
        <record id="sequence_partner_dist" model="ir.sequence">
            <field name="name">Sequence.partner.dist</field>
            <field name="code">dist.partner</field>
            <field name="prefix">D/</field>
			<field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <!-- séquence client Avis & communiqués-->
        <record id="sequence_partner_avis" model="ir.sequence">
            <field name="name">Sequence.partner.avis</field>
            <field name="code">avis.partner</field>
            <field name="prefix">A/</field>
			<field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_utm_investigation" model="ir.sequence">
            <field name="name">Sequence.crm.lead.partner.imp</field>
            <field name="code">survey.investigation.code</field>
            <field name="prefix">Enq/</field>
			<field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_mail_activity" model="ir.sequence">
            <field name="name">Mail Activity Sequence</field>
            <field name="code">mail.activity.code</field>
            <field name="active">TRUE</field>
            <field name="prefix">A</field>
			<field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>
