<odoo>
    <data>

<!--ADD THEME Action Menu Tree view -->
        <record id="utm_suspect_tree_view" model="ir.ui.view">
            <field name="name">survey.suspect.tree</field>
            <field name="model">survey.suspect</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Suspect">
                    <field name="name"/>
                    <field name="contact"/>
                    <field name="email"/>
                    <field name="address"/>
                    <field name="function"/>
                </tree>
            </field>
        </record>
        <!-- Action Menu Form view -->
        <record id="utm_suspect_form_view" model="ir.ui.view">
            <field name="name">survey.suspect.form</field>
            <field name="model">survey.suspect</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Suspect" version="7.0" duplicate="0" delete="0">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" required="1"/>
                                <field name="contact" required="1"/>
                                <field name="contact2" required="1"/>
                                <field name="address"/>
                                <field name="suspect_campaign_id"/>
                                <field name="suspect_study_id"/>
                            </group>
                            <group>
                                <field name="email"/>
                                <field name="function"/>
                                <field name="city"/>
                                <field name="country"/>
                                <field name="attribute" invisible="0"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Action Menu Item Related To Above Action Window -->
        <record id="action_utm_suspect_menu" model="ir.actions.act_window">
            <field name="name">Suspect</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">survey.suspect</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p>
                    Suspect
                </p>
            </field>
        </record>

        <menuitem id="utm_suspect_menu"
                  name="Suspect"
                  parent="survey_configuration_menu"
                  action="action_utm_suspect_menu"
                  sequence="11"
        />


    </data>
</odoo>
