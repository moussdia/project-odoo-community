<odoo>
    <data>
    <!--ADD THEME Action Menu Tree view -->
        <record id="utm_center_interest_tree_view" model="ir.ui.view">
			<field name="name">utm.center.interest.tree</field>
			<field name="model">utm.center.interest</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Centre d'intérêt" editable="bottom" default_order="id desc">
					<field name="name" required="1"/>
                    <field name="description"/>
				</tree>
			</field>
		</record>

        <!-- Action Menu Item Related To Above Action Window -->
        <record id="action_utm_center_interest_menu" model="ir.actions.act_window">
            <field name="name">Centre d'intérêt</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">utm.center.interest</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p>
                    Centre d'intérêt
                </p>
            </field>
        </record>

         <menuitem id="utm_center_interest_menu"
                  name="Centre d'intérêt"
                  parent="survey_configuration_menu"
                  action="action_utm_center_interest_menu"
                  sequence="33"
        />

    </data>
</odoo>
